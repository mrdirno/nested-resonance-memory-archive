{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://github.com/mrdirno/nested-resonance-memory-archive/schemas/pc002_comparative_results.json",
  "title": "PC002 Comparative Results Data Schema",
  "description": "JSON schema for comparative experimental data for PC002 (Transcendental Substrate Hypothesis). Compares transcendental (π,e,φ) vs PRNG substrates.",
  "type": "object",
  "required": ["metadata", "transcendental_results", "prng_results"],
  "properties": {
    "metadata": {
      "type": "object",
      "description": "Experiment metadata and provenance",
      "required": ["experiment_id", "pc_id"],
      "properties": {
        "experiment_id": {
          "type": "string",
          "description": "Unique comparative experiment identifier",
          "pattern": "^[A-Z0-9_]+$"
        },
        "pc_id": {
          "type": "string",
          "description": "Principle Card ID",
          "const": "PC002"
        },
        "domain": {
          "type": "string",
          "description": "Scientific domain",
          "default": "nested_resonance_memory"
        },
        "timestamp": {
          "type": "string",
          "description": "ISO 8601 timestamp",
          "format": "date-time"
        },
        "n_transcendental": {
          "type": "integer",
          "description": "Number of transcendental substrate experiments",
          "minimum": 1
        },
        "n_prng": {
          "type": "integer",
          "description": "Number of PRNG substrate experiments",
          "minimum": 1
        }
      }
    },
    "transcendental_results": {
      "type": "object",
      "description": "Aggregated results from transcendental substrate experiments",
      "required": ["pattern_lifetime", "memory_retention", "cluster_stability", "complexity"],
      "properties": {
        "pattern_lifetime": {
          "type": "object",
          "description": "Pattern lifetime distribution metrics",
          "required": ["mean", "std", "median"],
          "properties": {
            "mean": {"type": "number", "minimum": 0},
            "std": {"type": "number", "minimum": 0},
            "median": {"type": "number", "minimum": 0},
            "min": {"type": "number", "minimum": 0},
            "max": {"type": "number", "minimum": 0}
          }
        },
        "memory_retention": {
          "type": "number",
          "description": "Pattern recall accuracy percentage",
          "minimum": 0,
          "maximum": 100
        },
        "cluster_stability": {
          "type": "number",
          "description": "Coefficient of variation in cluster lifetimes",
          "minimum": 0
        },
        "complexity": {
          "type": "number",
          "description": "Emergent structure complexity score",
          "minimum": 0
        }
      }
    },
    "prng_results": {
      "type": "object",
      "description": "Aggregated results from PRNG substrate experiments",
      "required": ["pattern_lifetime", "memory_retention", "cluster_stability", "complexity"],
      "properties": {
        "pattern_lifetime": {
          "type": "object",
          "description": "Pattern lifetime distribution metrics",
          "required": ["mean", "std", "median"],
          "properties": {
            "mean": {"type": "number", "minimum": 0},
            "std": {"type": "number", "minimum": 0},
            "median": {"type": "number", "minimum": 0},
            "min": {"type": "number", "minimum": 0},
            "max": {"type": "number", "minimum": 0}
          }
        },
        "memory_retention": {
          "type": "number",
          "description": "Pattern recall accuracy percentage",
          "minimum": 0,
          "maximum": 100
        },
        "cluster_stability": {
          "type": "number",
          "description": "Coefficient of variation in cluster lifetimes",
          "minimum": 0
        },
        "complexity": {
          "type": "number",
          "description": "Emergent structure complexity score",
          "minimum": 0
        }
      }
    },
    "statistical_tests": {
      "type": "object",
      "description": "Statistical test results",
      "properties": {
        "pattern_lifetime_p": {
          "type": "number",
          "description": "p-value from Mann-Whitney U test",
          "minimum": 0,
          "maximum": 1
        },
        "memory_retention_p": {
          "type": "number",
          "description": "p-value from t-test",
          "minimum": 0,
          "maximum": 1
        },
        "cluster_stability_p": {
          "type": "number",
          "description": "p-value from F-test",
          "minimum": 0,
          "maximum": 1
        },
        "complexity_p": {
          "type": "number",
          "description": "p-value from MANOVA",
          "minimum": 0,
          "maximum": 1
        }
      }
    }
  },
  "examples": [
    {
      "metadata": {
        "experiment_id": "PC002_TRANS_VS_PRNG_N50",
        "pc_id": "PC002",
        "domain": "nested_resonance_memory",
        "timestamp": "2025-11-01T12:00:00Z",
        "n_transcendental": 50,
        "n_prng": 50
      },
      "transcendental_results": {
        "pattern_lifetime": {
          "mean": 125.5,
          "std": 25.3,
          "median": 120.0,
          "min": 80.0,
          "max": 200.0
        },
        "memory_retention": 75.5,
        "cluster_stability": 0.20,
        "complexity": 0.85
      },
      "prng_results": {
        "pattern_lifetime": {
          "mean": 95.2,
          "std": 30.1,
          "median": 90.0,
          "min": 50.0,
          "max": 150.0
        },
        "memory_retention": 60.2,
        "cluster_stability": 0.32,
        "complexity": 0.72
      },
      "statistical_tests": {
        "pattern_lifetime_p": 0.003,
        "memory_retention_p": 0.001,
        "cluster_stability_p": 0.01,
        "complexity_p": 0.005
      }
    }
  ]
}
